{
  "expo": {
    "name": "Simple Journal",
    "slug": "simple-journal",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Single screen app made to try out react native. Learning from expo and react-native docs.",
    "githubUrl": "https://github.com/sneakySensei/Simple-Journal-React-Native"
  }
}
